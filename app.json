{
  "expo": {
    "name": "Modem Diagnostics",
    "slug": "modem-diagnostics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2196F3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": [
      {
        "asset": "./assets/fonts/Manrope-Regular.ttf",
        "family": "Manrope-Regular"
      },
      {
        "asset": "./assets/fonts/Manrope-Bold.ttf",
        "family": "Manrope-Bold"
      },
      {
        "asset": "./assets/fonts/Manrope-Medium.ttf",
        "family": "Manrope-Medium"
      },
      {
        "asset": "./assets/fonts/Manrope-SemiBold.ttf",
        "family": "Manrope-SemiBold"
      },
      {
        "asset": "./assets/fonts/Manrope-Light.ttf",
        "family": "Manrope-Light"
      },
      {
        "asset": "./assets/fonts/Manrope-ExtraLight.ttf",
        "family": "Manrope-ExtraLight"
      },
      {
        "asset": "./assets/fonts/Manrope-ExtraBold.ttf",
        "family": "Manrope-ExtraBold"
      }
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2196F3"
      },
      "package": "com.krishnajayanth.modemdiagnostics",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "POST_NOTIFICATIONS",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b0c5cdd6-32a9-40d4-91a0-dc3d53ebf42d"
      }
    }
  }
}
